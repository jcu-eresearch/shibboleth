msgid "plugins.generic.shibboleth.displayName"
msgstr "Shibboleth Authentication Plugin"

msgid "plugins.generic.shibboleth.description"
msgstr "This plugin allows third-party authentication via a Shibboleth service."

msgid "plugins.generic.shibboleth.manager.settings.description"
msgstr "<p>With this plugin enabled, a Shibboleth single sign-on service can be used to register and authenticate users. You must have set up and configured the local service provider (SP).</p>"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethWayfUrl"
msgstr "Shibboleth SP path. This can be an absolute URL (e.g. https://example.com/Shibboleth.sso/Login) or a relative path (e.g. /Shibboleth.sso/Login)."

msgid "plugins.generic.shibboleth.manager.settings.shibbolethWayfUrlRequired"
msgstr "Please enter the path of the Shibboleth wayfinder service on this server. (e.g. Shibboleth.sso/Login)"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethHeaderUin"
msgstr "HTTP Header ($_SERVER environment variable key) with Shibboleth UIN"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethHeaderUinRequired"
msgstr "Please enter the header in which the Shibboleth UIN is found."

msgid "plugins.generic.shibboleth.manager.settings.shibbolethHeaderFirstName"
msgstr "HTTP Header ($_SERVER environment variable key) with Shibboleth first or given name"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethHeaderFirstNameRequired"
msgstr "Please enter the header in which the Shibboleth given name is found."

msgid "plugins.generic.shibboleth.manager.settings.shibbolethHeaderLastName"
msgstr "HTTP Header ($_SERVER environment variable key) with Shibboleth last, family, or surname"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethHeaderLastNameRequired"
msgstr "Please enter the header in which the Shibboleth surname is found."

msgid "plugins.generic.shibboleth.manager.settings.shibbolethHeaderInitials"
msgstr "HTTP Header ($_SERVER environment variable key) with Shibboleth personal initials"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethHeaderEmail"
msgstr "HTTP Header ($_SERVER environment variable key) with Shibboleth e-mail address"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethHeaderEmailRequired"
msgstr "Please enter the header in which the Shibboleth e-mail address is found."

msgid "plugins.generic.shibboleth.manager.settings.shibbolethHeaderPhone"
msgstr "HTTP Header ($_SERVER environment variable key) with Shibboleth telephone number"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethHeaderMailing"
msgstr "HTTP Header ($_SERVER environment variable key) with Shibboleth postal mailing address"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethAdminUins"
msgstr "List of Shibboleth user IDs or UINs who are OJS administrators"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethAdminUinsRequired"
msgstr "Please enter at least one administratorâ€™s Shibboleth UIN."

msgid "plugins.generic.shibboleth.manager.settings.shibbolethOptional"
msgstr "Optional Shibboleth Login"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethOptionalDescription"
msgstr "Selecting this option gives user the option of logging in with either a local account or a Shibboleth account. This is useful if your journal will have guest users who don't have a Shibboleth account but would still like to contribute."

msgid "plugins.generic.shibboleth.manager.settings.shibbolethOptionalLabel"
msgstr "Optional Shibboleth Login"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethOptionalTitle"
msgstr "Label for Shibboleth Login (default is Institutional Login)"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethOptionalDescriptionForLogin"
msgstr "Description for Shibboleth Login (default is blank)"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethOptionalDescriptionForRegistration"
msgstr "Description for Shibboleth Registration (default is blank)"

msgid "plugins.generic.shibboleth.manager.settings.shibbolethOptionalButtonLabel"
msgstr "Label for Shibboleth Login Button (default is Institutional Login)"

msgid "plugins.generic.shibboleth.manager.settings.InstitutionalLogin"
msgstr "Institutional Login"

msgid "plugins.generic.shibboleth.manager.settings.LocalLogin"
msgstr "Local Login"

msgid "plugins.generic.shibboleth.manager.settings.Register"
msgstr "Register via"
